{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch WebExtension",
        "type": "firefox",
        "request": "launch",
        "reAttach": true,
        "addonPath": "${workspaceFolder}/extension",
        "pathMappings": [
          {
            "url": "moz-extension://2ec818aa-f06c-4d88-b3eb-b2f7a86b5837",
            "path": "${workspaceFolder}/extension"
          }
        ]
      },
      {
        "name": "Attach WebExtension",
        "type": "firefox",
        "request": "attach",
        "addonPath": "${workspaceFolder}/extension",
        "pathMappings": [
          {
            "url": "moz-extension://2ec818aa-f06c-4d88-b3eb-b2f7a86b5837",
            "path": "${workspaceFolder}/extension"
          }
        ]
      }
    ]
}
